\begin{equation}
M_X = 
\left(
\begin{array}{c}
30 \\
40
\end{array}\right),
\quad
D_X = 
\left(
\begin{array}{cc}
30 \cdot 30 & 0 \\
0 & 30 \cdot 30
\end{array}\right);
\end{equation}
\begin{equation}
M_{\nu} = 
\left(
\begin{array}{c}
0 \\
 0
\end{array}\right),
\quad
D_{\nu} = 
\left(
\begin{array}{cc}
\left(\frac{5 \cdot \pi}{180} \right)^2 & 0 \\
0 & 30 \cdot 30
\end{array}\right).
\end{equation}

